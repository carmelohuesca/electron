{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "comma-dangle": 2,
        "curly": 2,
        "default-case": 2,
        "dot-notation": 2,
        "eqeqeq": 2,
        "guard-for-in": 2,
        "no-alert": 2,
        "no-caller": 2,
        "no-cond-assign": 2,
        "no-console": 0,
        "no-control-regex": 2,
        "no-constant-condition": 2,
        "no-debugger": 2,
        "no-div-regex": 2,
        "no-dupe-keys": 2,
        "no-duplicate-case": 2,
        "no-else-return": 2,
        "no-eval": 2,
        "no-empty": 2,
        "no-empty-character-class": 2,
        "no-extra-boolean-cast": 2,
        "no-extra-parens": 2,
        "no-extra-semi": 2,
        "no-ex-assign": 2,
        "no-func-assign": 2,
        "no-inner-declarations": 2,
        "no-irregular-whitespace": 2,
        "no-invalid-regexp": 2,
        "no-multi-str": 2,
        "no-negated-in-lhs": 2,
        "no-obj-calls": 2,
        "no-regex-spaces": 2,
        "no-sparse-arrays": 2,
        "no-script-url": 2,
        "no-self-compare": 2,
        "no-unreachable": 2,
        "no-unexpected-multiline": 2,
        "no-void": 2,
        "no-warning-comments": 2,
        "quotes": [
            "error",
            "single"
        ],
        "semi": 2,
        "strict": 2,
        "use-isnan": 2,
        "valid-jsdoc": 0,
        "valid-typeof": 2
    }
}